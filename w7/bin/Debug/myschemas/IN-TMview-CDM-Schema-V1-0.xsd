		</xs:sequence>
	</xs:complexType>

	<!-- ====================================================================== -->
	<!-- Filename    : FormattedNameAddressType.xsd                             -->
	<!-- Author      : TM-XML WG                                                -->
	<!-- Version     : 1.0                                                      -->
	<!-- Last Update : 2016-02-22                                               -->
