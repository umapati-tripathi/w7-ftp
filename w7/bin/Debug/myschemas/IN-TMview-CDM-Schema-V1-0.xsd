			<xs:element name="Address" type="AddressType" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>

	<!-- ====================================================================== -->
	<!-- Filename    : FreeFormatNameType.xsd                                   -->
	<!-- Author      : TM-XML WG                                                -->
	<!-- Version     : 1.0                                                      -->
	<!-- Last Update : 2016-02-22                                               -->
	<!-- ====================================================================== -->
	<xs:complexType name="FreeFormatNameType">
		<xs:sequence>
			<xs:element name="FreeFormatNameDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FreeFormatNameLine" type="xs:string" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!-- ====================================================================== -->
	<!-- Filename    : GoodsServicesType.xsd                                    -->
	<!-- Author      : TM-XML WG                                                -->
	<!-- Version     : 1.0                                                      -->
	<!-- Last Update : 2016-02-22                                               -->
